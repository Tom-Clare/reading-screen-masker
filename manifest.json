{
    "name": "Rose",
    "description": "Screen masker to aid reading for those with difficuluty focusing",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting", "tabs"],
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
            "128": "assets/logo128.png"
        }
    },
    "icons": {
        "128": "assets/logo128.png"
    }
}